<!-- Header
================================================== -->
<nav class="wp-header clearfix" id="top" itemprop="WPHeader" role="banner">
    <div class="container">
        <ul class="nav visuallyhidden">
            <li><a href="#hero">Skip to Content</a></li>
            <li><a href="#nav">Skip to Navigation</a></li>
            <li><a href="/medicare">Return to Homepage</a></li>
        </ul>
        <div class="clearfix">
            <ul class="nav" itemscope itemtype="http://schema.org/Organization">
                <li class="nav-brand">
                    <a class="brand logo-regence-cs" href="$theme_display.getURLHome()">
                        <span class="brand-logo"><span class="hide-text">${ch_brand}</span></span>
                        <span class="brand-region">#ch_region_text()</span>
                    </a>
                </li>
                #if($is_signed_in)
                    <li class="nav-login"><a href="$theme_display.getURLSignOut()">Sign out</a></li>
                #else
                    <li class="nav-login"><a href="$theme_display.getURLSignIn()">Sign in</a></li>
                #end
                <li class="nav-shopping">
                    <a href="#" data-target="#set-view-location" data-toggle="collapse" class="toggle shopping-toggle-button">#ch_location_audience()<small>#ch_location_text()</small></span><span aria-hidden="true" class="glyph-users hidden-phone"></span><b class="caret"></b></a>
                </li>
                <li class="nav-search">
                    <form class="form-search" action="http://www.regence.com/about/search.jsp" method="get" name="header-search" id="search" role="search">
                        <label for="searchQuery" class="hide">Search</label>
                        <input type="text" id="searchQuery" name="searchQuery" placeholder="Search" class="input-medium search-query">
                        <button type="submit" class="btn btn-link"><span class="glyph-search" aria-hidden="true"></span><span class="visuallyhidden">Go</span></button>
                    </form>
                </li>
            </ul>
        </div>
    </div>
</nav>
<header id="set-view-location" class="subhead collapse out">
    <div class="container">
        <form>
            <div class="row-fluid">
                <div class="span6">
                    <i class="glyph-users"></i>
                    <label>Change audience</label>
                    <select>
                        <option>Individuals & Families</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                    </select>
                </div>
                <div class="span6">
                    <i class="icon-location icon-white"></i>
                    <label>Change location</label>
                    <input type="text" placeholder="Postal code">
                </div>
            </div>
            <div class="form-actions">
                <button type="button" class="btn btn-link">Cancel</button>
                <button type="submit" class="btn">Update</button>
            </div>
        </form>
        #set ($VOID = $velocityPortletPreferences.setValue('portlet-setup-show-borders', 'true'))
        $theme.runtime("setviewlocation_WAR_setviewlocation001SNAPSHOT","",$velocityPortletPreferences.toString())
    </div>
</header>            